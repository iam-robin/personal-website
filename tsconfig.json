{
    "extends": "astro/tsconfigs/strict",
    "include": [".astro/types.d.ts", "**/*"],
    "exclude": ["dist"],
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@components/*": ["src/components/*"],
            "@assets/*": ["src/assets/*"],
            "@layouts/*": ["src/layouts/*"],
            "@utils/*": ["src/utils/*"]
        }
    }
}
